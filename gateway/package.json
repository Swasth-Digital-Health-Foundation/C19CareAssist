{
  "name": "healthgateway",
  "version": "1.0.0",
  "description": "Health Gateway: A broker to negotiate communication between user facing apps and HSPs",
  "main": "build/start.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "node build/server.js",
    "test": "NODE_ENV=test LOG_LEVEL=error jest --runInBand --forceExit --coverage",
    "start-dev": "nodemon --config \"./nodemon.json\"/",
    "dev": "NODE_ENV=DEV npm run build && nodemon --config \"./nodemon.json\"/",
    "build": "rm -rf ./build && tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "pkg": {
    "assets": [
      "env.DEV"
    ]
  },
  "bin": "./build/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bharathealthstack/healthgateway.git"
  },
  "author": "Bharathealthstack developer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bharathealthstack/healthgateway/issues"
  },
  "homepage": "https://github.com/bharathealthstack/healthgateway/#readme",
  "dependencies": {
    "@overnightjs/core": "^1.6.4",
    "@overnightjs/logger": "^1.1.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/lodash": "^4.14.150",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "kafka-node": "^5.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "node-rsa": "^1.0.8",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rollbar": "^2.17.0",
    "ts-node": "^8.1.0",
    "typeorm": "^0.2.32",
    "typescript": "^3.6.3",
    "validatorjs": "^3.18.1",
    "winston": "^3.2.1",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.23",
