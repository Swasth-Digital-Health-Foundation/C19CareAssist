    "dockerpkg": "^1.0.0",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "nodemon": "^1.18.11",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "tslib": "^2.3.0",
    "tslint": "^5.15.0"
  }
